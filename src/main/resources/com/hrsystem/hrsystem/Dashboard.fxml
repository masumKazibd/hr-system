                <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="nameColumn" prefWidth="225.0" text="Name" />
                <TableColumn fx:id="departmentColumn" prefWidth="200.0" text="Department" />
                <TableColumn fx:id="salaryColumn" prefWidth="199.0" text="Salary" />
            </columns>
        </TableView>
        <Label layoutX="50.0" layoutY="65.0" text="Employee Snapshot:">
